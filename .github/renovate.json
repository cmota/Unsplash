{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":dependencyDashboard"
  ],
  "labels": [
    "dependency-update"
  ],
  "rebaseWhen": "conflicted",
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "semanticCommitType": "chore",
      "matchUpdateTypes": [
        "major",
        "minor",
        "patch"
      ],
      "automerge": false,
      "automergeType": "branch",
      "ignoreTests": true
    }
  ]
}
